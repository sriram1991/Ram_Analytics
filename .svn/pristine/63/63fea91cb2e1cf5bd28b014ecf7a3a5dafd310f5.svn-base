<link rel="stylesheet" type="text/css" href="/static/css/common/student_reg_form.css">

<div class="container"><!-- /.container -->

  
    <div class="wrapper row-offcanvas row-offcanvas-left">  
    <!-- ========== Student Registration Form Begin ============================== -->
